#include<stdio.h>
int main()
{
	printf("The food items are\nFood item - Pizza\nPrice - Rs 239\nFood item - Burger\nPrice - Rs 129\nFood item - Pasta\nPrice - Rs 179\nFood item - French Fries\nPrice - Rs 99\nFood item - Sandwich\nPrice - Rs 149\n");
	int choice;
	scanf("%d",choice);
	switch(choice)
	   {
	   case 1:
		printf("Food item - Pizza\nPrice - Rs 239\n");
		break;
	   case 2:
		printf("Food item - Burger\nPrice - Rs 129\n");
		break;
	   case 3:
		printf("Food item - Pasta\nPrice - Rs 179\n");
		break;
	   case 4:
		printf("Food item - French Fries\nPrice - Rs 99\n");
		break;
	   case 5:
		printf("Food item - Sandwich\nPrice - Rs 149\n");
	    break;
	  default:
	    	printf("Your choice is invalid");
	  }
		
		
return 0;	     	
			
}
