#include<stdio.h>
int prime(int num);
int prime(int num)
{
	int i;
	int r=sqrt(num);
	for(i=2;i<=r;i++)
	{
		if(num%i==0)
		{
			return 1;
		}
	}
}
int main()
{
	int num,check;
	printf("Enter the number :");
	scanf("%d",&num);
	check =  prime(num);
	if(check==1)
	{
		printf("The given number is not a prime number");
	}
	else
	{
		printf("Tht given number is a prime number");
	}
}
